    "require 'rbplotly'\n",
    "\n",
    "trace1 = { x: [1,2,3,4,5,6], y: [100,200,300,400,500,600] }\n",
    "trace2 = { x: [1,2,3,4,5,6], y: [600,500,400,300,200,100] }\n",
    "traces = [trace1, trace2]\n",
    "pl = Plotly::Plot.new(data: traces)\n",
    "pl.show"
